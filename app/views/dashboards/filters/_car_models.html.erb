<% if @search.make_id.present? and @search.car_model_id.nil? and @search.car_models.any? %>
  <div class="panel panel-default">
    <div class="panel-heading"><h3 class="panel-title"><%= t ".car_models" %></h3>
    </div>
    <ul class="nav nav-pills nav-stacked">
      <% @search.car_models.each_with_index do |car_model, index| %>
        <%= render partial: 'searches/filters/row', 
          locals:{ value: car_model, name: "car_model", index: index} %>
      <% end %>
    </ul>
    <% if @search.car_models.size > 5 %>
      <a href="" class="show_more" ><%= t"see more" %></a>
    <% end %>
  </div>
<% end %>


