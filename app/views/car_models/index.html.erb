<h1>Listing car_models</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Make</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @car_models.each do |car_model| %>
      <tr>
        <td><%= car_model.name %></td>
        <td><%= car_model.make %></td>
        <td><%= link_to 'Show', car_model %></td>
        <td><%= link_to 'Edit', edit_car_model_path(car_model) %></td>
        <td><%= link_to 'Destroy', car_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car model', new_car_model_path %>
