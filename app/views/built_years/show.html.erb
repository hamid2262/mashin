<% @page_title = " اتویابی - خرید و فروش خودرو #{@car_model.make.name} #{@car_model.name} #{@built_year.year}"  %>
<% breadcrumb :built_year, @built_year %>

<div id="built_year_show" class="pages_with_search_form">
  <div id="search_list" class="col-sm-7 col-md-9">
    <%= render 'shared/ads_list' %>
  </div> 

  <div id="search_bar" class="col-sm-5 col-md-3">
    <%= render 'shared/search_form' %>
    <%= render 'built_years/show/built_year_car_details' %>
  </div>
</div>

<% if can? @car_model, :edit %>
  <%= link_to 'Edit', edit_make_car_model_built_year_path %> |
<% end %>
